<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Content</th>
    <th>Creator</th>
    <th>Attach</th>
    <th>Origin feed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.id %></td>
    <td><%= feed.content %></td>
    <td><%= feed.creator_id %></td>
    <td><%= feed.attach_id %></td>
    <td><%= feed.origin_feed_id %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
